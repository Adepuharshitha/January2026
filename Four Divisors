
from typing import List
import math

class Solution:
    def sumFourDivisors(self, nums: List[int]) -> int:
        def isPrime(x):
            if x < 2:
                return False
            for i in range(2, int(math.sqrt(x)) + 1):
                if x % i == 0:
                    return False
            return True

        total_sum = 0

        for n in nums:
            count = 0
            div_sum = 0

            for d in range(1, int(math.sqrt(n)) + 1):
                if n % d == 0:
                    d2 = n // d
                    if d == d2:
                        count += 1
                        div_sum += d
                    else:
                        count += 2
                        div_sum += d + d2

                if count > 4:
                    break

            if count == 4:
                total_sum += div_sum

        return total_sum
